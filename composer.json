{
  "name": "paulthebaud/phpunit-generator",
  "description": "A PHPUnit6 tests skeletons generator",
  "homepage": "https://github.com/paul-thebaud/phpunit-generator",
  "version": "1.1.1",
  "license": "MIT",
  "authors": [
    {
      "name": "Paul Th√©baud",
      "email": "paul.thebaud29@gmail.com"
    }
  ],
  "keywords": [
    "php",
    "phpunit",
    "generator",
    "unit",
    "test",
    "tests"
  ],
  "bin": [
    "phpunitgen"
  ],
  "require": {
    "php": "^7.0",
    "twig/twig": "^2.0",
    "nikic/php-parser": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0.8",
    "squizlabs/php_codesniffer": "^2.8.1",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "PHPUnitGenerator\\": "src/PHPUnitGenerator/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHPUnitGenerator\\": "test/PHPUnitGenerator/"
    }
  },
  "scripts": {
    "cs-check": "php ./vendor/bin/phpcs",
    "cs-fix": "php ./vendor/bin/phpcbf",
    "phpunit" : "php ./vendor/bin/phpunit",
    "test" : [
      "@cs-check",
      "@phpunit"
    ]
  }
}
