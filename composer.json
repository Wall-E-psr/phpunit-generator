{
  "name": "paulthebaud/phpunit-generator",
  "description": "A PHPUnit7 tests skeletons generator",
  "homepage": "https://github.com/paul-thebaud/phpunit-generator",
  "version": "2.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Paul Th√©baud",
      "email": "paul.thebaud29@gmail.com"
    }
  ],
  "keywords": [
    "php",
    "php7",
    "phpunit",
    "generator",
    "unit",
    "test",
    "tests"
  ],
  "bin": [
    "phpunitgen"
  ],
  "require": {
    "php": "~7.2",
    "league/flysystem": "~1.0",
    "nikic/php-parser": "~3.0",
    "symfony/console": "~4.0",
    "symfony/yaml": "~4.0",
    "twig/twig": "~2.0",
    "psr/container": "~1.0.0",
    "respect/validation": "~1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "squizlabs/php_codesniffer": "~2.8",
    "satooshi/php-coveralls": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "PhpUnitGen\\": "src/PhpUnitGen/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpUnitGen\\": "test/PhpUnitGen/"
    }
  },
  "scripts": {
    "cs-check": "php ./vendor/bin/phpcs",
    "cs-fix": "php ./vendor/bin/phpcbf",
    "phpunit": "php ./vendor/bin/phpunit",
    "test": [
      "@cs-check",
      "@phpunit"
    ]
  }
}
